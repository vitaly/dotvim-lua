{
  "config function": {
    "prefix": ["conf"],
    "body": [
      "config = function()",
      "\t$0",
      "end,"
    ]
  },

  "return config object": {
    "prefix": ["rconf"],
    "body": [
      "return {",
      "\tconfig = function()",
      "\t\t$0",
      "\tend,",
      "}"
    ]
  },

  "setup function": {
    "prefix": ["set"],
    "body": [
      "setup = function()",
      "\t$0",
      "end,"
    ]
  },

  "return setup object": {
    "prefix": ["rset"],
    "body": [
      "return {",
      "\tsetup = function()",
      "\t\t$0",
      "\tend,",
      "}"
    ]
  },

  "wmap": {
    "prefix": ["wmap"],
    "body": [
      "require('which-key').register {",
      "\t['<leader>$0'] = {",
      "\t\t$1 = { '$2', '$3' },",
      "\t},",
      "}"
    ]
  }
}
